MODEL_TYPE(PHA)
//g = 9.8067 taken from wikipedia
#define g 9.8
#define alpha 0.7854
[0,100]Sy;
[-10,1000]Sx;
[0,15]t;
[0,15]time;
U(0,100)v0;
{
mode1;
invt_c:
(Sx<100.0);
flow:
d/dt[Sx]=v0 * cos(alpha);
d/dt[Sy]=v0 * sin(alpha) - g * t;
d/dt[t]=1.0;
jump:
(and(t > 0.000001) (Sy = 0))==>@1(and (Sx' = Sx) (Sy' = 0) (t' = 0) (v0' = 0.9 * v0));
}
init:
@1(and(Sx=0)(Sy=0)(t=0));
goal:
@1(and(Sx>=100.0)(Sy>=0));
goal_c:
@1(and(Sy<=0.0)(t>0.000001));

