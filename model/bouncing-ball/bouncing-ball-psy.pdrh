MODEL_TYPE(PSY)
//g = 9.8067 taken from wikipedia
#define g 9.8
//#define alpha 0.7854
[0.0, 0.7584] alpha;
[0,100]Sy;
[-10,1000]Sx;
[0,15]tau;
[0,15]time;
[19,21]v0;
{
mode1;
invt_c:
(Sx<100.0);
flow:
d/dt[Sx]=v0 * cos(alpha);
d/dt[Sy]=v0 * sin(alpha) - g * tau;
d/dt[tau]=1.0;
jump:
(and(tau > 0.000001) (Sy = 0))==>@1(and (Sx' = Sx) (Sy' = 0) (tau' = 0) (v0' = 0.9 * v0));
}
init:
@1(and(Sx=0)(Sy=0)(tau=0));