[0.7854] alpha;
[9.8] g;
[0, 100] Sy;
[0, 1000] Sx;
[0, 15] t;
[0, 15] time;
N(20.0, 3.0) v0;

{ mode 1;
  
  flow:
        d/dt[Sx] = v0 * cos(alpha);
        d/dt[Sy] = v0 * sin(alpha) - g * t;
        d/dt[t] = 1.0;
  jump:
        (and (t > 0.000001) (Sy = 0)) ==> @1 (and (Sx' = Sx) (Sy' = 0) (t' = 0) (v0' = 0.9 * v0));
}

init:
@1    (and 
			(Sx = 0)
			(Sy = 0)
			(t = 0)
		);

goal:
@1    (and (Sx >= 100.0) (Sy >= 0));

